function [sat_time_locations] = import_sat_pos_file(file_name,num_header_lines)

sat_time_locations = [];

fileID = fopen(file_name,'r');

% throw away header lines
for i=1:num_header_lines
    tline = fgetl(fileID);
end

for i=1:1000
    tline = fgetl(fileID);
    [t1,t2,t3,t4,x,y,z] = sscanf(tline,'%s %s %s %s   % 12.6f    % 12.6f    % 12.6f \n');

    sat_time_locations = [sat_time_locations; x y z];
end